{
  "name": "exstreme/assetfree-yii2",
  "description": "A meta package that allows you to install yii2 without composer-asset-plugin. Fork",
  "type": "library",
  "require": {
    "php": ">=7.4",
    "yiisoft/yii2": "=2.0.45"
  },
  "replace": {
    "bower-asset/jquery": "*",
    "bower-asset/jquery.inputmask": "*",
    "bower-asset/inputmask": "*",
    "bower-asset/punycode": "*",
    "bower-asset/yii2-pjax": "*"
  },
  "conflict": {
    "bower-asset/jquery": "*",
    "bower-asset/jquery.inputmask": "*",
    "bower-asset/inputmask": "*",
    "bower-asset/punycode": "*",
    "bower-asset/yii2-pjax": "*"
  }
}